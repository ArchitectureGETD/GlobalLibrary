    <Authors>GlobalExchange</Authors>
    <Company>GlobalExchange</Company>
    <Product>iTextSharp.GE.xtra</Product>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <Import Project="..\VersionInfo.proj" />

  <ItemGroup>
    <None Include="..\VersionInfo.proj" Link="VersionInfo.proj" />
  </ItemGroup>

  <ItemGroup>
